{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/config-screen.html",
      "dest": "/dist/config-screen.html"
    },
    {
      "src": "/flag-selector.html",
      "dest": "/dist/flag-selector.html"
    },
    {
      "src": "/index.js",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
