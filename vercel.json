{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/**/*.mjs",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/launchdarkly.mjs": {
      "runtime": "nodejs18.x"
    },
    "api/test.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/flag-selector.html",
      "dest": "/flag-selector.html"
    },
    {
      "src": "/index.js",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
