{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/launchdarkly.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/launchdarkly",
      "dest": "/api/launchdarkly.js"
    },
    {
      "src": "/flag-selector.html",
      "dest": "/flag-selector.html"
    },
    {
      "src": "/index.js",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
